{
    "name": "cloudcreativity/demo-laravel-json-api",
    "description": "Demo Laravel application using the cloudcreativity/laravel-json-api package.",
    "keywords": [
        "laravel",
        "json-api",
        "cloudcreativity"
    ],
    "license": "Apache-2.0",
    "type": "project",
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "cloudcreativity/laravel-json-api": "^1.5",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.6.*",
        "laravel/passport": "^6.0",
        "laravel/tinker": "^1.0"
    },
    "require-dev": {
        "cloudcreativity/json-api-testing": "^1.1",
        "filp/whoops": "^2.1",
        "fzaninotto/faker": "~1.4",
        "laravel/homestead": "^9.2",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoload": true
    }
}
